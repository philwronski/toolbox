{
  "name": "toolbox",
  "version": "1.0.0",
  "description": "toolbox",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --port 3000",
    "build": "webpack --config webpack.config.js",
    "lint:css": "stylelint **/*.scss",
    "prettier": "prettier --write",
    "test": "npm run lint:css"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:css"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{scss}": [
        "lint:css",
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*.min.css"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint-config-prettier": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-airbnb": "0.0.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
